<div class="container">
	<div class="row">
		<h2>GENERATED WITH YEOMAN</h2>

		<%if (gallery === true) { %>
			{% plugin gallery gallery-name %}
			{% marker gallery-name %}
			    {% for entry in gallery-name.entries %}
			        {{ entry.body }}<br>
			        {{ entry.description }}
			        <hr>
			    {% endfor %}
			{% endmarker %}
		<% } %>
	</div>
</div>

<script type="text/javascript">
	if (!window.console) window.console={log:function(){}};

	(function($){
		$(document).ready(function(){
			// document is ready
		});
	})(jQuery);
</script>